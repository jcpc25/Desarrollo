@model COES.MVC.Intranet.Areas.Hidrologia.Models.PtoMedicionModel

<script src="~/Areas/Hidrologia/Content/Scripts/ptomededit.js"></script>
<script type="text/javascript">

    $(function () {
        $('#cbEmpresaPto').val($('#hfEmpresaPto').val());

        $('#cdPtomedibarranomb2').val($('#hfPtomedibarranomb2').val());
        $('#cdPtomedielenomb2').val($('#hfPtomedielenomb2').val());
        $('#cbFamilia').val($('#hfFamilia').val());
        $('#cbEquipo').val($('#hfEquipo').val());
        $('#cbOrigenLectura2').val($('#hfOrigenLectura2').val());      
        $('#cbTipoPtoMedicion2').val($('#hfTipoPtoMedicion2').val());

        var i = $('#hfTipoPtoMedicion2').val();
        alert(i);


        $('#cdOrden').val($('#hfOrden').val());
        $('#cdOsicodi').val($('#hfOsicodi').val());

    });

</script>
   




 @using (Html.BeginForm("Listar", "Evento", FormMethod.Post, new { @class = "form-search", name = "frmEditar", id = "frmEditar" }))
{ 
     <table style="width:100%" class="table-form-show">
        <tr>
             <td class="tbform-label">Descripción:</td>
             <td class="tbform-control">
                 <input type="Text" id="cdPtomedibarranomb2" name="idPtomedibarranomb2" value="" onkeydown="return validarLetras(event)"/>
                 <input type="hidden" id="hfPtomedibarranomb2" value="@Model.ptomedicion.Ptomedibarranomb"  />
                 <input type="hidden" id="hfPtomedicodi2" name="idPtomedicodi2" value="@Model.ptomedicion.Ptomedicodi" />
             </td>
        </tr>
        <tr>
             <td class="tbform-label">Abreviatura:</td>
             <td class="tbform-control">
                 <input type="Text" id="cdPtomedielenomb2" name="idPtomedielenomb2" value=""  onkeydown="return validarLetras(event)"/>
                 <input type="hidden" id="hfPtomedielenomb2" value="@Model.ptomedicion.Ptomedielenomb" />
             </td>
        </tr>
        <tr>
             <td class="tbform-label">Empresa:</td>
             <td class="tbform-control">
                 <select id="cbEmpresaPto" name="idEmpresa">
                    <option value="">--SELECCIONE--</option>
                    @foreach (var item in Model.ListaEmpresas)
                    {   
                        <option value="@item.EMPRCODI">@item.EMPRNOMB</option>
                    }
                 </select>
                 <input type="hidden" id="hfEmpresaPto" value="@Model.ptomedicion.Emprcodi" />
             </td>
        </tr>
        <tr>
             <td class="tbform-label">Tipo Equipo:</td>
             <td class="tbform-control">
                 <select id="cbFamilia" name="idFamilia">
                    <option value="">--SELECCIONE--</option>
                    @foreach (var item in Model.ListaFamilia)
                    {   
                        <option value="@item.Famcodi">@item.Famnomb</option>
                    }
                 </select>
                 <input type="hidden" id="hfFamilia" value="@Model.ptomedicion.famcodi" />
             </td>
        </tr>
        <tr>
             <td class="tbform-label">Equipo:</td>
             <td class="tbform-control">
                 <select id="cbEquipo" name="idEquipo">
                    <option value="">--SELECCIONE--</option>
                    @foreach (var item in Model.ListaEquipo)
                    {   
                        <option value="@item.Equicodi">@item.Equinomb</option>
                    }
                 </select>
                 <input type="hidden" id="hfEquipo" value="@Model.ptomedicion.Equicodi" />
             </td>
        </tr>
        <tr>
             <td class="tbform-label">Origen Lectura:</td>
             <td class="tbform-control">
                 <select id="cbOrigenLectura2" name="idOrigenLectura2">
                    <option value="">--SELECCIONE--</option>
                    @foreach (var item in Model.ListaOrigenLectura)
                    {   
                        <option value="@item.Origlectcodi">@item.Origlectnombre</option>
                    }
                 </select>
                 <input type="hidden" id="hfOrigenLectura2" value="@Model.ptomedicion.Origlectcodi" />
             </td>
        </tr>
        <tr>
             <td class="tbform-label">Tipo Punto de Medición:</td>
             <td class="tbform-control">
                 <select id="cbTipoPtoMedicion2" name="idTipoPtoMedicion2">
                    <option value="">--SELECCIONE--</option>
                    @foreach (var item in Model.ListaTipoPuntoMedicion)
                    {   
                        <option value="@item.Tipoptomedicodi">@item.Tipoptomedinomb</option>
                    }
                 </select>
                 <input type="hidden" id="hfTipoPtoMedicion2" value="@Model.ptomedicion.Tipoptomedicodi" />
             </td>
        </tr>
        <tr>
             <td class="tbform-label">Orden:</td>
             <td class="tbform-control">
                 <input type="Text" id="cdOrden" name="idOrden" onkeypress="return validarNumero(this,event)" value="0" />
                 <input type="hidden" id="hfOrden" value="@Model.ptomedicion.Orden" />
             </td>
        </tr>
        <tr>
             <td class="tbform-label">Código Osinerg:</td>
             <td class="tbform-control">
                 <input type="Text" id="cdOsicodi" name="idOsicodi" value="@Model.ptomedicion.Osicodi" onkeydown="return validarLetras(event)" />
                  <input type="hidden" id="hfOsicodi" value="@Model.ptomedicion.Osicodi" />
             </td>
        </tr>
         <tr>
             <td>.</td>
             
         </tr>
         <tr>
             <td></td>
             <td colspan="2">
                  <input type="button" id="btnGrabar" value="Grabar" />
                  <input type="button" id="btnCancelar" value="Cancelar" />
             </td>
         </tr>         
    </table>
 }


