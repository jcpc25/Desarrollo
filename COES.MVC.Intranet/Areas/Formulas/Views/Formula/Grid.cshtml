@model COES.MVC.Intranet.Areas.Formulas.Models.FormulaModel

<table border="0" class="pretty" id="tabla">
    <thead>
        <tr>
            <th>ID</th>
            <th>Subestación</th>
            <th>Área</th>
            <th>Día Típico</th>
            <th>Registro</th>
            <th>Fecha Inicial</th>
            <th>Fecha Fin</th>
            <th>Usuario</th>
            <th>Actualización</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Listado ) {
        <tr onclick="mostrarDetalle(@item.PERFCODI);" style="cursor:pointer">                        
            <td>@item.PERFCODI</td>
            <td>@item.PRRUABREV</td>
            <td>@item.PRRUNOMB</td>
            <td>@COES.MVC.Intranet.Helper.Tools.ObtenerClasificacion(@item.PERFCLASI)</td>
            <td>@(((DateTime)item.FECREGISTRO).ToString("dd/MM/yyyy"))</td>
            <td>@(((DateTime)item.FECINICIO).ToString("dd/MM/yyyy"))</td>
            <td>@(((DateTime)item.FECFIN).ToString("dd/MM/yyyy"))</td>
            <td>@item.LASTUSER</td>
            <td>@(((DateTime)item.LASTDATE).ToString("dd/MM/yyyy HH:mm"))</td>
        </tr>
    }
    </tbody>
</table>
