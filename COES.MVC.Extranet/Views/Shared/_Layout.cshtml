<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" />
    <link rel="icon" href="~/favicon.ico" />
    <meta name="viewport" content="width=device-width" />  

    <link href="~/Content/Css/css.css" rel="stylesheet" />
    <link href="~/Content/Css/menu.css" rel="stylesheet" />
    <link href="~/Content/Css/calendar.css" rel="stylesheet" />
    <link href="~/Content/Css/table.css" rel="stylesheet" />
    <link href="~/Content/Css/popup.css" rel="stylesheet" />
    <link href="~/Content/Css/form.css" rel="stylesheet" />

    @* CSS BOOTSTRAP *@
@*    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.css")" rel="stylesheet" type="text/css" /> *@
 

    <script type="text/javascript">
        var siteRoot = "@Url.Content("~/")";
        var initialURL = "http://localhost/AppExtranet";
    </script>

    <script src="~/Content/Scripts/jquery.js"></script>
    <script src="~/Content/Scripts/datepicker..js"></script>
    <script src="~/Content/Scripts/menu.js"></script>
    <script src="~/Content/Scripts/datatable.js"></script>
    <script src="~/Content/Scripts/efecto.js"></script>
    <script src="~/Content/Scripts/popup.js"></script>
    <script src="~/Content/Scripts/master.js"></script>      

    @* Js BOOTSTRAP *@
 @*   <script src="@Url.Content("~/Content/bootstrap/js/bootstrap.js")" type="text/javascript"></script> *@
</head>
<body>  
    <div class="layout">     
    
    @Html.Action("Header","Home", new {model = Model, area=""})       
    <div style="clear:both"></div>
     
    <div class="content">
         <div class="content-menu">
            @Html.Action("Menu", "Home", new { model = Model, area = "" })
        </div>
        <div style="clear:both"></div>
        <div id="loading" class="loading">
            <div class="loading-image"><img src="~/Content/Images/loading.gif" /></div>
            <div class="loading-text">Procesando...</div>
        </div>
        <section>           
            <div id="divGeneral">
                @RenderBody()
            </div>
            <div id="divTemporal" class="oculto">
                <iframe id="frameContenido" class="frame"></iframe>
                <div class="clear"></div>
            </div>
        </section>
    </div>   
    </div>
    <footer>
        <div class="footer">
            <span>COES © Todos los derechos reservados</span>
            <br />
            <span>C. Manuel Roaud y Paz Soldan 364. San Isidro, Lima - PERÚ Teléfono: (511) 611-8585</span>
        </div>
    </footer>
</body>
<script type="text/javascript">
        $(document).ready(function () {
            $('.menu').jqsimplemenu();
        });
</script>
</html>
